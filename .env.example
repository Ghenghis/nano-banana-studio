# =============================================================================
# NANO BANANA STUDIO PRO - ENVIRONMENT TEMPLATE
# =============================================================================
# Copy this file to .env and fill in your API keys
# NEVER commit .env with real keys to version control!
# =============================================================================

# =============================================================================
# AI MODEL API KEYS (Required - at least one)
# =============================================================================
OPENAI_API_KEY=sk-your-openai-api-key-here
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
GOOGLE_GENERATIVE_AI_API_KEY=your-google-ai-key-here
XAI_API_KEY=your-xai-key-here

# =============================================================================
# LOCAL LLM SERVICES (Optional)
# =============================================================================
OLLAMA_API_BASE_URL=http://localhost:11434
LMSTUDIO_API_BASE_URL=http://localhost:1234

# =============================================================================
# CLOUD SERVICES
# =============================================================================
HUGGINGFACE_TOKEN=hf_your-huggingface-token
OPENROUTER_API_KEY=your-openrouter-key

# Animation Providers (Optional)
RUNWAY_API_KEY=your-runway-key
KLING_API_KEY=your-kling-key
REPLICATE_API_KEY=your-replicate-key

# TTS Providers (Optional)
ELEVENLABS_API_KEY=your-elevenlabs-key

# =============================================================================
# MUSIC GENERATION
# =============================================================================
SUNO_COOKIE=your-suno-cookie
TWOCAPTCHA_API_KEY=your-2captcha-key

# =============================================================================
# DATABASE & CACHE
# =============================================================================
REDIS_URL=redis://localhost:6379
POSTGRES_PASSWORD=your-postgres-password
POSTGRES_USER=postgres

# Supabase (Optional)
SUPABASE_URL=http://localhost:5432
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters

# =============================================================================
# PATHS (Docker defaults)
# =============================================================================
UPLOAD_DIR=/app/data/uploads
OUTPUT_DIR=/app/data/outputs
CACHE_DIR=/app/data/cache
MODEL_DIR=/app/models

# =============================================================================
# LOGGING & DEBUG
# =============================================================================
LOG_LEVEL=INFO
VITE_LOG_LEVEL=info

# =============================================================================
# WEB SERVER
# =============================================================================
PORT=8000

# =============================================================================
# CONTEXT SETTINGS (for local LLMs)
# =============================================================================
DEFAULT_NUM_CTX=12288
